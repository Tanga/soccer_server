# this represents the behavior of objects that want to prented to be domain objects
RSpec.shared_examples "a Domain <%= head_name.capitalize %>" do |parameter|
  describe 'attributes' do
    subject do
      described_class.new(id: 1)
    end

    it 'fill in some domain behavior'
    describe '#id' do
      it { expect(subject.id).to eq 1 }
    end
  end
end
